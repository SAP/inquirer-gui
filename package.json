{
  "name": "@sap-devx/inquirer-gui",
  "displayName": "Inquirer GUI",
  "version": "3.0.0",
  "description": "Inquirer Graphical User Interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/inquirer-gui"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Eyal Barlev",
    "email": "eyal.barlev@sap.com"
  },
  "publisher": "SAP",
  "main": "dist/form.umd.js",
  "module": "./dist/form.es.js",
  "exports": {
    ".": {
      "import": "./dist/form.es.js",
      "require": "./dist/form.umd.js"
    },
    "./dist/form.css": {
      "import": "./dist/form.css",
      "require": "./dist/form.css"
    }
  },
  "scripts": {
    "build": "vite build",
    "build-dev": "vite build --mode development",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "test": "jest -w 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -V -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true,
      "es2022": true,
      "document": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {},
    "root": true
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "strip-ansi": "^6.0.0",
    "vue": "^3.3.4",
    "vuetify": "^3.3.10"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "2.4.0",
    "@vue/vue3-jest": "^29.2.4",
    "babel-jest": "^29.5.0",
    "coveralls": "2.13.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "6.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "sass": "^1.63.6",
    "vite": "^4.3.9",
    "yorkie": "^2.0.0"
  }
}
