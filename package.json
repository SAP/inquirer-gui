{
  "name": "@sap-devx/inquirer-gui",
  "displayName": "Inquirer GUI",
  "version": "0.5.0",
  "description": "Inquirer Graphical User Interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/inquirer-gui"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Eyal Barlev",
    "email": "eyal.barlev@sap.com"
  },
  "publisher": "SAP",
  "main": "dist/form.umd.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name form src/index.js",
    "build-dev": "vue-cli-service build --mode development",
    "build-for-sample": "vue-cli-service build --target lib --name form src/index.js --dest sample-app/form",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run build",
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit -w 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "root": true
  },
  "dependencies": {
    "strip-ansi": "^6.0.0",
    "vue": "2.6.12",
    "vuetify": "2.3.10"
  },
  "devDependencies": {
    "@babel/preset-env": "7.14.1",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "4.5.12",
    "@vue/test-utils": "1.2.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "coveralls": "2.11.16",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-serializer-vue": "^2.0.2",
    "sass": "1.37.2",
    "sass-loader": "10.0.2",
    "vue-cli-plugin-vuetify": "2.4.1",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.6.12",
    "vuetify-loader": "1.7.2",
    "webpack": "4.46.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
