{
  "name": "@sap-devx/inquirer-gui",
  "displayName": "Inquirer GUI",
  "version": "0.1.8",
  "publisher": "SAP",
  "author": {
    "name": "Eyal Barlev",
    "email": "eyal.barlev@sap.com"
  },
  "description": "Inquirer Graphical User Interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/inquirer-gui"
  },
  "license": "Apache-2.0",
  "main": "dist/form.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name form src/index.js",
    "build-for-sample": "vue-cli-service build --target lib --name form src/index.js --dest sample-app/form",
    "build-dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "strip-ansi": "^6.0.0",
    "vue": "^2.6.10",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^4.1.1",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "jest-serializer-vue": "^2.0.2",
    "sass": "^1.24.3",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.3",
    "webpack": "^4.41.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
