{
	"name": "sample-inquirer-gui-vscode-extension",
	"displayName": "Inquirer GUI",
	"description": "",
	"publisher": "SAP",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/SAP/inquirer-gui/tree/master/sample-vscode-extension"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:inquirerUiExample.show"
