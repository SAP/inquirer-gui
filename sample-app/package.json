{
  "name": "sample-app",
  "version": "1.1.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build": "vue-cli-service build",
    "build-dev": "vue-cli-service build --mode development --disable-plugin @vue/cli-plugin-eslint",
    "lint": "vue-cli-service lint",
    "prep-local": "cd .. && npm run build-for-sample",
    "prepublishOnly": "npm run build",
    "serve": "vue-cli-service serve --disable-plugin @vue/cli-plugin-eslint",
    "test": "vue-cli-service test:unit",
    "copy-inquirer-gui": "copyfiles -V -f ../dist/* ./node_modules/@sap-devx/inquirer-gui/dist && copyfiles -f ../package.json ./node_modules/@sap-devx/inquirer-gui",
    "copy-autocomplete-plugin": "copyfiles -V -f ../auto-complete-plugin/dist/* ./node_modules/@sap-devx/inquirer-gui-auto-complete-plugin/dist && copyfiles -f ../auto-complete-plugin/package.json ./node_modules/@sap-devx/inquirer-gui-auto-complete-plugin/",
    "prebuild": "npm run prep-local && npm run copy-inquirer-gui && npm run copy-autocomplete-plugin",
    "clean": "rm -rf ./dist ./form"
  },
  "dependencies": {
    "@mdi/font": "^4.9.95",
    "@sap-devx/inquirer-gui": "^0.6.0",
    "@sap-devx/inquirer-gui-date-plugin": "0.0.2",
    "@sap-devx/inquirer-gui-file-browser-plugin": "0.0.5",
    "@sap-devx/inquirer-gui-label-plugin": "0.0.1",
    "@sap-devx/inquirer-gui-login-plugin": "0.0.4",
    "@sap-devx/inquirer-gui-tiles-plugin": "0.0.8",
    "@sap-devx/inquirer-gui-radio-plugin": "0.0.1",
    "@sap-devx/inquirer-gui-single-checkbox-plugin": "0.0.1",
    "@sap-devx/inquirer-gui-auto-complete-plugin": "0.0.1",
    "@sap-devx/webview-rpc": "^0.3.1",
    "material-design-icons-iconfont": "^5.0.1",
    "vue": "2.6.12",
    "vuetify": "2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-vuetify": "2.2.2",
    "vue-template-compiler": "2.6.12",
    "vuetify-loader": "1.7.2",
    "webpack": "^4.41.2"
  }
}
