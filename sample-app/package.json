{
  "name": "sample-app",
  "version": "1.1.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "scripts": {
    "build": "vue-cli-service build",
    "build-dev": "vue-cli-service build --mode development --disable-plugin @vue/cli-plugin-eslint",
    "prep-local": "cd .. && npm run build-for-sample",
    "prepublishOnly": "npm run build",
    "serve": "vue-cli-service serve --disable-plugin @vue/cli-plugin-eslint",
    "test": "vue-cli-service test:unit",
    "copy-inquirer-gui": "copyfiles -V -f ../dist/* ./node_modules/@sap-devx/inquirer-gui/dist && copyfiles -f ../package.json ./node_modules/@sap-devx/inquirer-gui",
    "copy-autocomplete-plugin": "copyfiles -V -f ../auto-complete-plugin/dist/* ./node_modules/@sap-devx/inquirer-gui-auto-complete-plugin/dist && copyfiles -f ../auto-complete-plugin/package.json ./node_modules/@sap-devx/inquirer-gui-auto-complete-plugin/",
    "prebuild": "npm run prep-local && npm run copy-inquirer-gui && npm run copy-autocomplete-plugin",
    "clean": "rm -rf ./dist ./form"
  },
  "dependencies": {
    "@mdi/font": "^4.9.95",
    "@sap-devx/webview-rpc": "^0.3.1",
    "material-design-icons-iconfont": "^5.0.1",
    "vue": "^3.3.4",
    "vuetify": "^3.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass-loader": "^13.3.2"
  }
}
