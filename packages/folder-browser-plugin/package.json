    "build-dev": "vite build --mode development",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/inquirer-gui/tree/master/folder-browser-plugin"
